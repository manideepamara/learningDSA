
    //with O(n*n) time and O(n) space
    int jump(vector<int>& nums) {
        int n = nums.size();
        int dp[n];
        memset(dp,-1,sizeof(dp));
        dp[0]=0;
        for(int i=0;i<n;i++){
            if(dp[i]!=-1){
                for(int j=i+1;j<n &&j<=nums[i]+i;j++){
                    dp[j]=dp[j]==-1?dp[i]+1:min(dp[j],dp[i]+1);
                }
            }
        }
        return dp[n-1];
    }
